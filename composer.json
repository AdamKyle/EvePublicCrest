{
    "name": "eveonline/eve-market-details",
    "type": "library",
    "description": "EveOnline is a public crest API library that makes fetching api end points easier.",
    "keywords": ["framework", "http", "fetching", "api", "eveonline"],
    "homepage": "http://adambalan.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Balan",
            "email": "adamkylebalan@gmail.com",
            "homepage": "https://github.com/eve-online"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "~6.0",
        "monolog/monolog": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": "5.1.7",
        "phpunit/php-code-coverage": "^3"
    },
    "autoload": {
        "psr-4": {
            "EveOnline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EveOnline\\Tests\\": "tests/"
        }
    }
}
